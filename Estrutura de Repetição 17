import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    int num, fatorial = 1;

    System.out.print("Digite um número: ");
    num = input.nextInt();

    for (int i = num; i > 0; i--) {
      fatorial *= i;
    }

    System.out.println("O fatorial de " + num + " é " + fatorial);
  }


}
