import java.util.Scanner;


public class Main {
  public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    int num = 0;
    System.out.println("Números ímpares entre 1 e 50:");
    while (num < 50) {
      num++;
      if (num % 2 != 0) {
        System.out.println(num);
      }
    }
  }


}
