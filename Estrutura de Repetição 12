import java.util.Scanner;



public class Main {
  public static void main(String[] args) {
    Scanner teclado = new Scanner(System.in);
    int num;
    System.out.println("Digite um n√∫mero: ");
    num = teclado.nextInt();
    for(int i = 1; i <= 10; i++){
      System.out.println(num + " x " + i + " = " + num * i);
    }

  }


}
